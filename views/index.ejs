                    <% data.Peripherals.forEach((per) => {%>
                      <%if (per.type==="mouse") {%>
                      <%=per.brand + " " + per.name %>
                      <% }%>
                      <% }) %>
                  </a></span></h2>
              <div class="e1285-8 x-bar-content-area">
                <ul class="x-block-grid four-up">
                  <li class="x-block-grid-item">
                    <h3 style="margin: 0 0 0.35em; font-size: 1em; letter-spacing: 0.05em; line-height: 1;">DPI</h3>

                    <p class="man"><%=data.dpi%></p>
                  </li>
                  <li class="x-block-grid-item">
                    <h4 style="margin: 0 0 0.35em; font-size: 1em; letter-spacing: 0.05em; line-height: 1;">Sensitivity
                    </h4>

                    <p class="man"><%=data.sensitivity%></p>
                  </li>
                  <li class="x-block-grid-item">
                    <h4 style="margin: 0 0 0.35em; font-size: 1em; letter-spacing: 0.05em; line-height: 1;">eDPI</h4>

                    <p class="man"><%=data.edpi%></p>
                  </li>

                  <li class="x-block-grid-item">
